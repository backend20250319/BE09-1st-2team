<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="insertMenu">
        INSERT INTO menu (menu_category_id, menu_name, price)
        VALUES (?, ?, ?)
    </entry>
    <entry key="updateMenu">
        UPDATE menu
        SET
            menu_name = ?
            , price = ?
            , is_sold_out = ?
        WHERE menu_id = ?
    </entry>
    <entry key="deleteMenu">
        DELETE
        FROM menu
        WHERE menu_id = ?
    </entry>
</properties>